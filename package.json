{
  "name": "express-boilerplate-multitenant",
  "author": "PIYoung (https://github.com/PIYoung)",
  "version": "1.0.0",
  "description": "node express postgres boilerplate for multitenant",
  "license": "MIT",
  "main": "src/server.ts",
  "sideEffects": false,
  "homepage": "https://github.com/PIYoung/node-express-boilerplate-multitenant#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PIYoung/node-express-boilerplate-multitenant.git"
  },
  "bugs": {
    "url": "https://github.com/PIYoung/node-express-boilerplate-multitenant/issues"
  },
  "scripts": {
    "start": "node -r dotenv/config dist/src/server.js dotenv_config_path=.env.production",
    "start:dev": "nodemon -r dotenv/config src/server.ts dotenv_config_path=.env.development",
    "build": "tsc -p tsconfig.json",
    "test": "jest -i --colors --detectOpenHandles",
    "test:unit": "jest -i --colors --detectOpenHandles --testMatch=\"<rootDir>/test/unit/**/*.spec.js\"",
    "test:e2e": "jest -i --colors --detectOpenHandles --testMatch=\"<rootDir>/test/e2e/**/*.spec.js\"",
    "test:watch": "jest -i --watchAll",
    "test:coverage": "jest -i --coverage",
    "prepare": "husky install",
    "pretty": "prettier --write \"./**/*.{js,jsx,json,ts,tsx}\"",
    "lint": "eslint --ignore-path .gitignore --ext .ts src/",
    "lint:fix": "eslint --ignore-path .gitignore --ext .ts src/ --fix",
    "lint:staged": "lint-staged"
  },
  "lint-staged": {
    "*.ts": [
      "npm run pretty",
      "npm run lint:fix"
    ]
  },
  "dependencies": {
    "app-root-path": "^3.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "joi": "^17.7.0",
    "jsonwebtoken": "^9.0.0",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "sequelize": "^6.28.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.16",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "@types/passport-local": "^1.0.35",
    "@types/supertest": "^2.0.12",
    "cross-env": "^7.0.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.2",
    "supertest": "^6.3.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
