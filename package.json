{
  "name": "express-boilerplate-multitenant",
  "author": "PIYoung (https://github.com/PIYoung)",
  "version": "1.0.0",
  "description": "node express postgres boilerplate for multitenant",
  "license": "ISC",
  "main": "src/server.js",
  "homepage": "https://github.com/PIYoung/node-express-boilerplate-multitenant#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PIYoung/node-express-boilerplate-multitenant.git"
  },
  "bugs": {
    "url": "https://github.com/PIYoung/node-express-boilerplate-multitenant/issues"
  },
  "scripts": {
    "start": "node -r dotenv/config src/server.js dotenv_config_path=.env.production --exec babel-node",
    "start:dev": "cross-env NODE_ENV=development nodemon -r dotenv/config src/server.js dotenv_config_path=.env.development  --delay 2",
    "test": "jest -i --colors --detectOpenHandles",
    "test:unit": "jest -i --colors --detectOpenHandles --testMatch=\"<rootDir>/test/unit/**/*.spec.js\"",
    "test:e2e": "jest -i --colors --detectOpenHandles --testMatch=\"<rootDir>/test/e2e/**/*.spec.js\"",
    "test:watch": "jest -i --watchAll",
    "test:coverage": "jest -i --coverage",
    "prepare": "husky install",
    "pretty": "prettier --write \"./**/*.{js,jsx,json,ts,tsx}\"",
    "lint": "eslint --ignore-path .gitignore --ext .js src/"
  },
  "dependencies": {
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.7.0",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pg": "^8.8.0",
    "sequelize": "^6.28.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.16",
    "@types/node": "^18.11.18",
    "cross-env": "^7.0.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.3",
    "jest": "^29.3.1",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.2",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  }
}
